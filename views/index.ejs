<!DOCTYPE html>
<html lang="en">
<%- include('partials/head') %>
<body>
  <%- include('partials/nav') %>
  <% messages.forEach((message) => { %>
    <a href=<%= `/messages/${message.id}` %>>
      <h3><%= message.user %></h3>
      <small><%= message.added.toDateString() %></small>
      <p><%= message.text %></p>
    </a>
  <% }) %>
</body>
</html>